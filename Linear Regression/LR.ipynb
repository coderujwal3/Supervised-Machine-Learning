   "source": [
    "from sklearn.linear_model import LinearRegression\n",
    "\n",
    "model = LinearRegression()\n",
    "X = [[1],[2],[3],[4],[5]]\n",
    "y = [40,50,65,75,90]\n",
    "\n",
    "model.fit(X,y)\n",
    "\n",
    "hours = float(input(\"Enter the numbers of hours you studied: \"))\n",
    "marks = model.predict([[hours]])[0]\n",
    "\n",
    "print(f\"{hours} hours: {marks} marks\")"
   ]
  }
